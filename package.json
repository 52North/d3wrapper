{
  "name": "d3wrapper",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-prod": "ng build --prod --output-hashing=none",
    "build-dev": "ng build --prod --output-hashing=none --aot=false --build-optimizer=false",
    "concat": "cat dist/d3wrapper/runtime-es2015.js dist/d3wrapper/polyfills-es2015.js dist/d3wrapper/main-es2015.js > preview/d3TsWrapper.js",
